<apex:page title="Generate Pdf" renderAs="pdf" standardController="Quotation__c" extensions="BAFCOGeneratePDFController" applyHtmlTag="false" standardStylesheets="false" showHeader="false" applyBodyTag="false" action="{!getQuoteData}" >
    
    <head>
        <style >
            @page {
            size: a4 portrait;
            margin: 0px 0px 12px 0px;
            @top-center {
            content: element(header);
            } 
            @bottom-center {
            content: element(footer);
            width:100%
            }
            margin-top: 75px;
            }
            div.header {
            padding: 12px;
            position: running(header);
            }
            div.footer {
            display: block;
            padding:0px 0px 0px 0px;
            position: running(footer);
            }
            table{ width:100%;}
            @font-face {
            font-family: "MontserratCustomfont";
            src: url('{!URLFOR($Resource.Montserrat, 'static/Montserrat-Medium.ttf')}') format('truetype');
            }
            * {
            font-family: Helvetica,Dialog;
            font-size: 12px;
            }
            .boxesCss{
            max-width:168px;
            min-width:168px;
            width:168px;
            min-height:40px;
            max-height:40px;
            height:40px;
            //text-align: center;
            // background: #E5F6FE !important;
            //color : #01648E;
            }
        </style>
    </head>
