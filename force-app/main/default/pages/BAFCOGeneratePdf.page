                                <apex:outputText rendered="{!item.placeOfPickup  != NULL}"><b>Place of Pickup :</b></apex:outputText><apex:outputText rendered="{!item.placeOfPickup  != NULL}" value="{!item.placeOfPickup}"/>
                                <b>Transit Time :</b><apex:outputText value="** not Mapped **"/>
                                <b>Port of Destination :</b><apex:outputText value="{!item.portOfDestination}"/>
                                <apex:outputText rendered="{!showShippLine == true}"><b>ShippingLine :</b></apex:outputText> <apex:outputText rendered="{!showShippLine == true}" value="{!item.shippingLine}"/>
                            </apex:panelGrid>
                            <apex:panelGrid rendered="{!item.serviceType == 'D2D'}" columns="6">
                                <b>Port of Loading :</b><apex:outputText value="{!item.portOfLoading}"/>
                                <apex:outputText rendered="{!item.placeOfPickup  != NULL}"><b>Place of Pickup :</b></apex:outputText><apex:outputText rendered="{!item.placeOfPickup  != NULL}" value="{!item.placeOfPickup}"/>
                                <b>Transit Time :</b><apex:outputText value="** not Mapped **"/>
                                <b>Port of Destination :</b><apex:outputText value="{!item.portOfDestination}"/>
                                 <apex:outputText rendered="{!item.placeOfDischarge  != NULL}"><b>Place of Discharge :</b></apex:outputText><apex:outputText rendered="{!item.placeOfDischarge  != NULL}" value="{!item.placeOfDischarge}"/> 
                                <apex:outputText rendered="{!showShippLine == true}"><b>ShippingLine :</b></apex:outputText> <apex:outputText rendered="{!showShippLine == true}" value="{!item.shippingLine}"/>
                                
                            </apex:panelGrid>
